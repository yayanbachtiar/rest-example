{
  "name": "express-play",
  "version": "1.0.0",
  "description": "A boilerplate for Node.js, Express, Mongoose, Heroku, mLab, Nodemon, PM2, and Babel.",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "start": "nodemon src/app.js --exec babel-node",
    "build": "babel src -d dist --ignore **/__tests__/**,**/__mocks__/**",
    "lint": "eslint 'src/**/*.js'",
    "unit": "jest \"src/.*\\.js$\" --coverage --forceExit",
    "e2e": "cross-env JEST_ENV=e2e jest \"src/.*\\.js$\" --forceExit",
    "_codecov": "codecov",
    "_heroku": "heroku"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Express-Play.git"
  },
  "keywords": [
    "Node.js"
  ],
  "author": "Shyam Chen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Express-Play/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Express-Play#readme",
  "dependencies": {
    "@grpc/proto-loader": "0.5.0",
    "@tensorflow/tfjs": "1.0.4",
    "@tensorflow/tfjs-node": "1.0.3",
    "amqp-connection-manager": "2.3.1",
    "amqplib": "0.5.3",
    "apollo-server-express": "2.4.8",
    "bcrypt": "3.0.6",
    "chalk": "2.4.2",
    "cloudinary": "1.14.0",
    "compression": "1.7.4",
    "connect-redis": "3.4.1",
    "cors": "2.8.5",
    "express": "4.16.4",
    "express-history-api-fallback": "2.2.1",
    "express-rate-limit": "3.4.0",
    "express-session": "1.16.1",
    "graphql": "14.2.1",
    "graphql-subscriptions": "1.1.0",
    "graphql-tag": "2.10.1",
    "grpc": "1.20.0",
    "helmet": "3.16.0",
    "ioredis": "4.9.0",
    "joi": "14.3.1",
    "jsdom": "14.0.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.11",
